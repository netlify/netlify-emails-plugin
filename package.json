{
  "name": "@netlify/plugin-emails",
  "version": "1.0.0",
  "description": "Scans the /email directory and sends email templates to the email service",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*",
    "manifest.yml"
  ],
  "dependencies": {
    "@netlify/functions": "^1.2.0",
    "husky": "^8.0.1",
    "tslib": "^2.4.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest"
  },
  "keywords": [
    "emails",
    "netlify-plugin"
  ],
  "author": "Lewis Thorley",
  "contributors": [
    "Lewis Thorley"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.10.0"
  },
  "devDependencies": {
    "@netlify/build": "^27.1.4",
    "@netlify/eslint-config-node": "^6.0.0",
    "@types/jest": "^28.1.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  },
  "postinstall": "husky install",
  "husky": {
    "hooks": {
      "pre-commit": "cross-env lint-staged echo \"[Husky] pre-commit\"",
      "pre-push": "cross-env lint-staged yarn build"
    }
  },
  "repository": "url - tbc",
  "bugs": {
    "url": "url/issues - tbc"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run test:staged"
    ],
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
